alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias cat='cat -n'
alias mkdir='mkdir -p'

alias ps='ps --srot=start_time'
alias which='type -a'
alias job='jobs -l'

alias du='du -kh'
alias df='df -kTh'

alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

alias ls='ls -CFh --color'
alias ll='ls -CFahl --color'
alias ..='cd ..'